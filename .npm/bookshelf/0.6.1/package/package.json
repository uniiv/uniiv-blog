    "pg": "~2.8.2",
    "sqlite3": "~2.1.7",
    "objectdump": "~0.3.0",
    "underscore.string": "~2.3.1",
    "grunt": "~0.4.1",
    "grunt-release": "~0.6.0",
    "mocha-as-promised": "~1.4.0",
    "chai-as-promised": "~4.1.0",
    "chai": "~1.8.0",
    "sinon-chai": "~2.4.0",
    "sinon": "~1.7.3"
